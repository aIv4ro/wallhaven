{
  "rewrites": [
    {
      "source": "/api/v1/:match*",
      "destination": "https://api.endpoint.url/:match*"
    }
  ],
  "headers": [
    {
      "source": "api/v1/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET"
        }
      ]
    }
  ]
}
